<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://ua.nure.album.nure.ua/entity"
           xmlns:tns="http://album.nure.ua/entity" elementFormDefault="qualified">

    <xs:include schemaLocation="Album.xsd"/>

    <xs:element name="albums">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="album" type="tns:album"
                             minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
